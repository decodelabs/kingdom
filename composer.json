{
    "name": "decodelabs/kingdom",
    "description": "Service container and management system",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",
        "psr/container": "^2.0",

        "decodelabs/exceptional": "^0.6.1"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7.0"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Kingdom\\": "src/Kingdom/"
        },
        "classmap": [
            "src/Kingdom.php",
            "src/KingdomTrait.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
